-- 3d stuff
-- field of view
fov = 0.2 -- 0.2 = 72 degrees
unit= abs(64/sin(fov*.5)) -- how big one block is
horizon = 32
resolution = 1
viewheight = 128
viewcenter = 64--viewheight/2
drawdist = 20

--camera plane info
xplane0 = cos(fov/2) 
yplane0 = sin(fov/2)
xplane1 = cos(-fov/2)
yplane1 = sin(-fov/2)
planelength = sqrt((xplane0-xplane1)^2 + (yplane0-yplane1)^2)
shortestdist = cos(fov/2)

function tan(ang)
	return (sin(ang)/cos(ang))
end

function rotate(x,y)
	local rx = x*cos(-cam.d) - y*sin(-cam.d)
	local ry = x*sin(-cam.d) + y*cos(-cam.d)
	return rx,ry
end

function rotateback(x,y)
	local rx = x*cos(cam.d) - y*sin(cam.d)
	local ry = x*sin(cam.d) + y*cos(cam.d)
	return rx,ry
end

function rotated(x,y,d)
 local rx = x*cos(d) - y*sin(d)
	local ry = x*sin(d) + y*cos(d)
	return rx,ry
end

function getrx(x,y)
	return x*cos(-cam.d) - y*sin(-cam.d)
end

function point2pix(x,y,z)
 local rx,ry = rotate(x,y)
 local dist = rx - cam.rx
	local scale = unit/dist
 
 local sx = (ry - cam.ry)*scale+64
 local sy = (cam.z-z)*scale + horizon

 return sx,sy,scale
end

function point2pixb(x,y,z)
 local rx,ry = rotate(x,y)
 local dist = rx - 8*cam.rx
	local scale = unit/dist
 
 --local sx = (ry - 8*cam.ry)*scale+64
 local sy = (8*cam.z-8*z)*scale + horizon

 return sy,scale
end

function make_cam()
 cam = {}
 cam.x = 63
 cam.y = 33
 cam.z = 2 --height is pos
 cam.gx = 5
 cam.gy = 5
 cam.d = 0.125
 cam.rx, cam.ry = rotate(cam.x,cam.y)
end

function move_cam()
	local rx,ry = rotate(player.x,player.y)
	if rx - cam.rx > 3.5 then
		cam.rx = rx - 3.5
	end
	if rx - cam.rx < 2 then
		cam.rx = rx - 2
	end

	cam.gx,cam.gy = rotateback(cam.rx,cam.ry)
	
	cam.x += (cam.gx-cam.x)/2
	cam.y += (cam.gy-cam.y)/2
end

function snap_cam()
	local vvx,vvy = rotateback(player.vx,player.vy)

	cam.x = player.x - 10*vvx
	cam.y = player.y - 10*vvy
	
	point_cam()
	rotate_cam()

	player.vx=1
	player.vy=0

end

function rotate_cam()
	cam.rx, cam.ry = rotate(cam.x,cam.y)
end

function point_cam()
	cam.d = atan2(player.x-cam.x,player.y-cam.y)
end


dflist = {35,30,25,20,18,
									16,14,12,10,8,7,6,5,
									4,3,2.5,2,1.5,1}
									
function map3d()

	local mapx0 = 8*cam.x+320*cos(cam.d)
	local	mapy0 = 8*cam.y+320*sin(cam.d)
	
	local py0,scale0 = point2pixb(mapx0,mapy0,0)
	
	local vx = cos(cam.d-0.25)
	local vy = sin(cam.d-0.25)
		
	for df in all(dflist) do
		df*=8
		local mapx = 8*cam.x+df*cos(cam.d)
		local mapy = 8*cam.y+df*sin(cam.d)
		
		local py,scale = point2pixb(mapx,mapy,0)
			
		local yscale = py-py0
		local ppy = py0
		
		local scalerange = scale-scale0
		local mrangex = mapx-mapx0
		local mrangey = mapy-mapy0
		
		while ppy < py do
			local step = (ppy-py0)/yscale
			local sc = scale0 + scalerange*step
			local mx = mapx0 + mrangex*step
			local my = mapy0 + mrangey*step
			tline(64,ppy,127,ppy,mx,my,vx/sc,vy/sc)
			tline(63,ppy,0,ppy,mx,my,-vx/sc,-vy/sc)
			ppy+=1
			if (ppy>128) break
		end
		if (ppy>128) break
		mapx0 = mapx
		mapy0 = mapy
		mapx00 = mapx10
		mapy00 = mapy10
		scale0 = scale
		py0 = py
	end
end

function spr3d(ix,x,y,z)
--z is foot height
	local px,py,s = point2pix(x,y,z+1)
	local sx=8*ix%128
	local sy=(ix\16)*8
	sspr(sx,sy,8,8,1+px-s*.5,py+1,s,s)
	local pxl = px - .5*s + rnd(s)
	local pyl = py -.7*s + rnd(2*s)
	line(pxl,max(pyl,py),pxl,min(pyl+.4*s,py+s),7)
end

function draw_bug(s)
	spr3d(3,s.x,s.y,0)
end

function draw_shadow(s)
	local px,py,s = point2pix(s.x,s.y,0)	
	ovalfill(px-.4*s,py-.2*s,px+.4*s,py+.2*s,0)
end

function wall3d(w)
	local px,py,s = point2pix(w.x,w.y,0)
	rectfill(px-.5*s,py,px+.5*s,py-s*3,14)
	ovalfill(px-.5*s,py+.3*s,px+.5*s,py-s*.3,14)
end

function sort(a,key)
	local key=key or 1
 for i=1,#a do
  local j = i
  while j > 1 and a[j-1][key] < a[j][key] do
   a[j],a[j-1] = a[j-1],a[j]
   j = j - 1
  end
 end
end

	
	
	

