	ovalfill(px1,py3-.15*s3,px2,py3+.15*s3,12)
end

grav = 0.05/8

function over_solid(s)
	if (mget(s.x+.4,s.y+.4)>0) return true
	if (mget(s.x-.4,s.y+.4)>0) return true
	if (mget(s.x+.4,s.y-.4)>0) return true
	if (mget(s.x-.4,s.y-.4)>0) return true
	return false
end

function update_player()
	local rx,ry = rotate(player.x,player.y)
	local drx,dry = 0,0
	
	if (btn(0)) dry = -1/8
	if (btn(1)) dry = 1/8
	if (btn(2)) drx = 1/8
	if (btn(3)) drx = -1/8
	
	if (dry!=0 and drx!=0) dry,drx = dry*.71,drx*.71
	
	player.dz -= grav
	
	if btnp(5) and player.floor then
		player.dz=1/8
		player.floor=false
	end
	
	player.oversolid = over_solid(player)
	body.oversolid = player.oversolid
	
	player.z += player.dz
	player.floor=false
	if player.z <= 0 and player.oversolid and player.z > -.4 then
		player.dz=0
		player.z=0
		player.floor=true
	end
	
	rx += drx
	ry += dry
	
	player.cyc=0
	if btn()&0b1111 > 0 then
		player.vx = drx
		player.vy = dry
		player.cyc = (t()%.33)*3
	end
	
	update_limbs()
	
	local ox,oy = rotateback(player.vx,player.vy)
	local od = atan2(ox,oy)
	
	player.x,player.y = rotateback(rx,ry)
	
	for e in all(player_e) do
		
		local xgoal,ygoal = rotated(e.x0+e.xa,e.y0+e.ya,od)
		
		e.x += (player.x+xgoal-e.x)/2
		e.y	+= (player.y+ygoal-e.y)/2
		e.z += (player.z+e.z0+e.za-e.z)/2
		
	end
end

function update_limbs()
	if player.floor then
		rleg.za = .2*abs(sin(player.cyc))
		lleg.za = .2*abs(sin(player.cyc))
		rleg.xa = .3*sin(player.cyc)
		lleg.xa = -.3*sin(player.cyc)
		
		body.za = .1*abs(sin(player.cyc))
				
		rarm.xa = -.2*sin(player.cyc)
		larm.xa = .2*sin(player.cyc)
		rarm.za = 0
		larm.za = 0
	else
		body.za = 0
		
		rleg.za = -player.dz
		lleg.za = -player.dz
		rleg.xa = -player.dz
		lleg.xa = -player.dz
		
		rarm.xa = -.1
		larm.xa = -.1
		rarm.za = -player.dz*2
		larm.za = -player.dz*2
	end
	reye.za,leye.za,head.za, goggles.za= body.za,body.za,body.za,body.za
end

function get_pickups()
	for p in all(pickups) do
		if abs(p.x-player.x) < 1 and
		abs(p.z-player.z) < 1 and
		abs(p.y-player.y) < 1 then
			del(pickups,p)
			del(elist,p)
			add(got_pickups,p)
			sfx(0)
			if (#pickups <= 0) make_portal()
		end
	end
	if portal then
		if abs(portal.x-player.x) < 1.5 and
		abs(portal.z-player.z) < 1.5 and
		abs(portal.y-player.y) < 1.5 then
			sfx(1)
			start_gamewin()
			return
		end
	end
end
