function restore_pickup(n)
	local n = n or 1
	for i=1,n,1 do
		if (#got_pickups <= 0) break
		local p = got_pickups[#got_pickups]
		del(got_pickups,p)
		add(pickups,p)
		add(elist,p)
	end
end
