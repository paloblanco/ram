-- effects
function make_lasers()
	lasers = {}
end

function generate_laser()
	local laserdmax=25
	local l = {}
	local xy = flr(rnd(2))
	if xy==0 then --x
		l.y = flr(cam.y) + (-25 + flr(rnd(laserdmax*2)))
		l.dx = -1 + 2*flr(rnd(2))
	 l.x = flr(cam.x) - l.dx*laserdmax
	 l.dy = 0
	else
		l.x = flr(cam.x) + (-25 + flr(rnd(laserdmax*2)))
		l.dy = -1 + 2*flr(rnd(2))
	 l.y = flr(cam.y) - l.dy*laserdmax
	 l.dx = 0		
	end
	l.z = -5 + 10*flr(rnd(2))
	add(lasers,l)
end

function update_lasers()
	for l in all(lasers) do
		l.x += l.dx
		l.y += l.dy
		if (abs(cam.y-l.y)>32) del(lasers,l)
		if (abs(cam.x-l.x)>32) del(lasers,l)
	end
	generate_laser()
end

function draw_lasers()
	for l in all(lasers) do
		local px,py,s = point2pix(l.x,l.y,l.z)
		local px2,py2,s2 = point2pix(l.x-l.dx*10,l.y-l.dy*10,l.z)
		if (py>0 and py2>0) line(px,py,px2,py2,7)	
	end
end
