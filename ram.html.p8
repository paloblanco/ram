function start_title()
	_update60 = update_title
	_draw = draw_title
end

function update_title()
	if (btnp(4) or btnp(5)) start_gameplay()
end

function start_gameplay()
	fade_out()
	init_gameplay()
	_update60 = update_gameplay
	_draw = draw_gameplay
	_update60()
	fade_in()
end

function init_gameplay()
	make_cam()
	make_player()
	-- hacks
	move_cam()
	point_cam()
	rotate_cam()
	--
	make_pickups()
	make_lasers()
	--
	make_map()
	
	elist = {}
	for e in all(player_e) do
		add(elist,e)
	end
	for e in all(pickups) do
		add(elist,e)
	end	
end

function update_gameplay()
	update_player()
	cam.d %= 1
	
	local df = 0
	--if (btn(2)) df = 1/8
	--if (btn(3)) df = -1/16
	
	local dx = df*cos(cam.d)
	local dy = df*sin(cam.d)
	
	cam.x += dx
	cam.y += dy
	
	move_cam()
	point_cam()
	rotate_cam()
	if (btnp(4)) snap_cam()
	update_lasers()
	
	for e in all(elist) do
		e.rx = getrx(e.x,e.y)
	end
	get_pickups()
	if (player.z < -2) reboot()
end

function draw_gameplay()
	cls(9)
	sort(elist,"rx")
	draw_lasers()
	map3d()
	for e in all(elist) do
		if e.rx-cam.rx>0.5 and mget(e.x,e.y)>0 then
			if (e.shad and e.oversolid) draw_shadow(e)
		end
	end
	for e in all(elist) do
		if (e.rx-cam.rx>0.5) e:draw()
	end
	print("memories:"..#pickups,1,1,7)	
end

function draw_title()
	cls()
	cprint("r-a-m",30,8)
	cprint("running",45,8)
	cprint("after",52,8)
	cprint("memories",59,8)
	cprint("pbg, for ld54",80,2)
end

function _init()
	start_title()
end

function fade_out()
	for r=0,66,3 do
		_draw()
		rectfill(64-r,64-r,64+r,64+r,13)
		flip()
	end
end

function fade_in()
	for r=66,0,-3 do
		_draw()
		rectfill(64-r,64-r,64+r,64+r,13)
		flip()
	end
end

function make_map()
	for xx=0,127,1 do
	local told=0
	for yy=0,63,1 do
		local t=mget(xx,yy)
		if t == 5 then
			make_pickup(xx+.5,yy+.5,0.2)
			mset(xx,yy,told)
			t=told
		elseif t == 21 then
			make_pickup(xx+.5,yy+.5,1.5)
			mset(xx,yy,told)
			t=told
		end
		told=t
	end
	end
end

