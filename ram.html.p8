function make_pickups()
	pickups = {}
	got_pickups={}
	portal=nil
--	add(pickups,{x=10,y=10,z=1.5})
	
	for p in all(pickups) do
		p.rx=0
		p.shad=true
		p.oversolid=true
		p.draw=draw_pickup
	end
end

function make_pickup(x,y,z)
	local p = {}
	p.x=x
	p.y=y
	p.z=z
	p.rx=0
	p.shad=true
	p.oversolid=true
	p.draw=draw_pickup
	add(pickups,p)
end

function draw_pickup(s)
	local sp = 5 + flr(t()*20)%5
	spr3d(sp,s.x,s.y,s.z)
end

function make_portal()
	portal = {}
	portal.x = 66
	portal.y=33
	portal.z=2
	portal.rx=0
	portal.shad=true
	portal.oversolid=true
	portal.draw = draw_portal
	add(elist,portal)
end

function draw_portal(p)
	local px,py,s = point2pix(p.x,p.y,p.z)
	local w = s*(.1+1*abs(sin(t())))
	ovalfill(px-w,py-1.5*s,px+w,py+1.5*s,rnd(16))
	oval(px-w,py-1.5*s,px+w,py+1.5*s,7)
	local rx0 = px-2*s+rnd(4*s)
	local rx1 = px-2*s+rnd(4*s)
	local ry0 = py-2*s+rnd(4*s)
	local ry1 = py-2*s+rnd(4*s)
	rectfill(rx0,ry0,rx1,ry1,7)
end
